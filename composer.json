{
    "name": "kevinpapst/adminlte-bundle-demo",
    "license": "MIT",
    "type": "project",
    "description": "Demo application for showcasing the AdminLTEBundle",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "aspera/xlsx-reader": "^0.7.4",
        "box/spout": "^3.0",
        "doctrine/doctrine-bundle": "^1.9",
        "dompdf/dompdf": "^0.8.3",
        "friendsofsymfony/user-bundle": "~2.0",
        "kevinpapst/adminlte-bundle": "~2.6",
        "knplabs/knp-menu-bundle": "^2.2",
        "neitanod/forceutf8": "^2.0",
        "nuovo/spreadsheet-reader": "^0.5.11",
        "phpoffice/phpspreadsheet": "1.0.0",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.1",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "^4.1",
        "symfony/maker-bundle": "^1.11",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "~4.2.0"
        }
    }
}
